 ///Program to toggle every character in string
#include <stdio.h>
#include<string.h>


int main()
{
    char string[15] = "aYdfeEEUIiuiOOP";
    int i=0;
    while(string[i]!='\0')
    {
        if(string[i]>='a' && string[i]<='z') 
        string[i]=string[i]-32;
        else if(string[i]>='A' && string[i]<='Z')
        string[i]=string[i]+32;
        i++;
        
    }
    string[i]='\0';
    printf("%s",string);
  

    return 0;
}
